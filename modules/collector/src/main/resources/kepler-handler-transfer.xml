<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
    http://www.springframework.org/schema/util
	http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<bean class="com.kepler.admin.collector.transfer.TransferHandler">
		<constructor-arg>
			<util:list>
				<ref bean="kepler.transfer.dimension" />
				<ref bean="kepler.transfer.dashboard" />
			</util:list>
		</constructor-arg>
	</bean>

	<bean id="kepler.transfer.dimension" class="com.kepler.admin.collector.transfer.DimensionHandler" init-method="init">
		<constructor-arg index="0" ref="kepler.mongo.config.transfer.minute" />
		<constructor-arg index="1" ref="kepler.mongo.config.transfer.hour" />
		<constructor-arg index="2" ref="kepler.mongo.config.transfer.day" />
	</bean>

	<bean id="kepler.transfer.dashboard" class="com.kepler.admin.collector.transfer.DashboardHandler" init-method="init">
		<constructor-arg index="0" ref="kepler.mongo.config.transfer.dashboard" />
	</bean>

</beans>